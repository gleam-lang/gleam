import gleam/json

pub type Wibble {
  Wibble(tuple: #(Int, Float, #(String, Bool)))
}

fn wibble_to_json(wibble: Wibble) -> json.Json {
  let Wibble(tuple:) = wibble
  json.object([
    #("tuple", json.preprocessed_array([
      json.int(tuple.0),
      json.float(tuple.1),
      json.preprocessed_array([
        json.string(tuple.2.0),
        json.bool(tuple.2.1),
      ]),
    ])),
  ])
}
