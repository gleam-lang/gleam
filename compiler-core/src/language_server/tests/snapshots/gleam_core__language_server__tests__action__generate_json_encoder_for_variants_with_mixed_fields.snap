fn encode_wibble(wibble: Wibble) -> json.Json {
  case wibble {
    Wibble -> json.object([
      #("type", json.string("wibble")),
    ])
    Wobble(..) -> json.object([
      #("type", json.string("wobble")),
      #("field", json.string(wibble.field)),
