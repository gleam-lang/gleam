import gleam/json as json_encoding

pub type Wibble {
  Wibble(a: Int, b: Float, c: String)
}

fn encode_wibble(wibble: Wibble) -> json_encoding.Json {
  json_encoding.object([
    #("a", json_encoding.int(wibble.a)),
    #("b", json_encoding.float(wibble.b)),
    #("c", json_encoding.string(wibble.c)),
  ])
}
