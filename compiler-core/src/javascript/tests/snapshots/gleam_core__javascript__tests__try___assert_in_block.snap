import { Ok, makeError } from "../gleam.mjs";

export function main(x) {
  let $ = (() => {
    if (!x.isOk()) return x;
    let z = x[0];

    return new Ok(z + 1);
  })();
  if (!$.isOk()) {
    throw makeError(
      "assignment_no_match",
      "my/mod",
      2,
      "main",
      "Assignment pattern did not match",
      { value: $ }
    )
  }
  let y = $[0];
  return y;
}

