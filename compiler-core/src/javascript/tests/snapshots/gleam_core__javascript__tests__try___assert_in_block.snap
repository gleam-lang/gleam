import { Ok } from "../gleam.js";

export function main(x) {
  let $ = (() => {
    if (x.type === "Error") return x;
    let z = x[0];

    return new Ok(z + 1);
  })();
  if (!$.isOk()) throw new Error("Bad match");
  let y = $[0];
  return y;
}

