import { makeError, divideInt, toBitArray, bitArraySliceToInt } from "../gleam.mjs";

const FILEPATH = "src/module.gleam";

export function main() {
  let $ = toBitArray([]);
  if ($.bitSize < 8 || $.bitSize !== 8 + divideInt($.byteAt(0), 8) - 1) {
    throw makeError(
      "let_assert",
      FILEPATH,
      "my/mod",
      3,
      "main",
      "Pattern match failed, no pattern matched the value.",
      { value: $, start: 19, end: 71, pattern_start: 30, pattern_end: 64 }
    )
  }
  let len = $.byteAt(0);
  let payload = bitArraySliceToInt($, 8, 8 + (divideInt(len, 8)) - 1, true, false);
  return $;
}
