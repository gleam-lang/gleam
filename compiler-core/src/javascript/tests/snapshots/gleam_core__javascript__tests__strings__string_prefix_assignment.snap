expression: "\npub fn go(x) {\n  case x {\n    \"Hello, \" as greeting <> name -> greeting\n    _ -> \"Unknown\"\n  }\n}\n"
---
export function go(x) {
  if (x.startsWith("Hello, ")) {
    let name = x.slice(7);
    let greeting = "Hello, ";
    return greeting;
  } else {
    return "Unknown";
  }
}
