import { makeError, bitArraySlice } from "../gleam.mjs";

function go(x) {
  let n = 16;
  if (!(x.bitSize == n)) {
    throw makeError(
      "let_assert",
      "my/mod",
      4,
      "go",
      "Pattern match failed, no pattern matched the value.",
      { value: x }
    )
  }
  let a = bitArraySlice(x, 0, n);
  return x;
}
