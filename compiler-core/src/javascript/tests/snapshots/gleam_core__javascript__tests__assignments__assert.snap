import { makeError } from "../gleam.mjs";

function go(x) {
  if (x !== 1) {
    throw makeError(
      "assignment_no_match",
      "my/mod",
      1,
      "go",
      "Assignment pattern did not match",
      { value: x }
    )
  }
  return x;
}

