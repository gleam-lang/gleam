import { makeError, toBitArray } from "../gleam.mjs";

function go(x) {
  let $ = toBitArray([1, 2, 3]);
  if (!(($.bitSize >= 8 && ($.bitSize - 8) % 8 === 0))) {
    throw makeError(
      "let_assert",
      "my/mod",
      3,
      "go",
      "Pattern match failed, no pattern matched the value.",
      { value: $ }
    )
  }
  let b = $.slice(8);
  return $;
}
