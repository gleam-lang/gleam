----- COMPILED JAVASCRIPT
import { bitArraySliceToInt } from "../gleam.mjs";

export function go(x) {
  let size = 3;
  if (
    size * 2 >= 0 &&
    x.bitSize === size * 2 &&
    bitArraySliceToInt(x, 0, size * 2, true, false) === 1
  ) {
    return 1;
  } else {
    return 2;
  }
}
