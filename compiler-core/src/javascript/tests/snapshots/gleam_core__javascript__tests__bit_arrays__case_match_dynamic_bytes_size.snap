import { bitArraySlice } from "../gleam.mjs";

export function go(x) {
  let n = 3;
  if (n >= 0 && x.bitSize === n * 8) {
    let a = bitArraySlice(x, 0, n * 8);
    return a;
  } else {
    return x;
  }
}
