----- COMPILED JAVASCRIPT
import { bitArraySliceToInt } from "../gleam.mjs";

export function go(x, n) {
  if (
    n >= 0 &&
    x.bitSize >= n &&
    x.bitSize === 16 + n &&
    bitArraySliceToInt(x, n, 8 + n, true, false) === 13 &&
      bitArraySliceToInt(x, 8 + n, 16 + n, true, false) === 10
  ) {
    return 1;
  } else {
    return 2;
  }
}
