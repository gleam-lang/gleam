import { bitArraySliceToInt } from "../gleam.mjs";

function go(x) {
  let n = -10;
  if (n >= 0 && x.bitSize === n) {
    let int = bitArraySliceToInt(x, 0, n, true, false);
    return int;
  } else {
    return 2;
  }
}
