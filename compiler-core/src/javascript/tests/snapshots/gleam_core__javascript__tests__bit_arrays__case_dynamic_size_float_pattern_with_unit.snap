import { bitArraySliceToFloat } from "../gleam.mjs";

function go(x) {
  let size = 3;
  if (
    size >= 0 &&
    x.bitSize === size * 2 &&
    bitArraySliceToFloat(x, 0, size * 2, true) === 1.3
  ) {
    return 1;
  } else {
    return 2;
  }
}
