import { makeError, bitArraySlice } from "../gleam.mjs";

function go(x) {
  let n = 3;
  if (n <= 0 || x.bitSize !== n * 8) {
    throw makeError(
      "let_assert",
      "my/mod",
      4,
      "go",
      "Pattern match failed, no pattern matched the value.",
      { value: x }
