export function main(xs) {
  if (xs.hasLength(1) && xs.head === 1) {
    let x = xs.head;
    return x;
  } else if (xs.hasLength(2) && xs.tail.head === 1) {
    let x = xs.tail.head;
    return x;
  } else {
    return 0;
  }
}

