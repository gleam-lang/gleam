export function main(xs) {
  if (xs?.[1]?.length === 0 && xs[0] === 1) {
    let x = xs[0];
    return x;
  } else if (xs?.[1]?.[1]?.length === 0 && xs[1][0] === 1) {
    let x = xs[1][0];
    return x;
  } else {
    return 0;
  }
}

