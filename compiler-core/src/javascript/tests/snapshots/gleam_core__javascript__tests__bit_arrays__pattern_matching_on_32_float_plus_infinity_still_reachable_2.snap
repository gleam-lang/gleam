import { bitArraySliceToFloat } from "../gleam.mjs";

export function go(x) {
  if (x.bitSize === 32) {
    if (Number.isFinite(bitArraySliceToFloat(x, 0, 32, true))) {
      return "Float";
    } else if (x.byteAt(0) === 127 && x.byteAt(1) === 128) {
      if (x.byteAt(2) === 0 && x.byteAt(3) === 0) {
        return "+Infinity";
      } else {
        return "Other";
      }
    } else {
      return "Other";
    }
  } else {
    return "Other";
  }
}
