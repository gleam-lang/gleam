import { makeError, bitArraySliceToFloat } from "../gleam.mjs";

function go(x) {
  let n = 1;
  if (
    n <= 0 ||
    x.bitSize < n ||
    x.bitSize < 64 + n ||
    bitArraySliceToFloat(x, n, n + 64, true) !== 1.1
  ) {
    throw makeError(
      "let_assert",
      "my/mod",
      4,
      "go",
      "Pattern match failed, no pattern matched the value.",
      { value: x }
    )
  }
  return x;
}
