import { makeError } from "../gleam.mjs";

function go(x) {
  if (!(x.bitSize == 40)) {
    throw makeError(
      "let_assert",
      "my/mod",
      3,
      "go",
      "Pattern match failed, no pattern matched the value.",
      { value: x }
    )
  }
  let a = x.sliceToFloat(0, 32, false);
  let b = x.byteAt(4);
  return x;
}
