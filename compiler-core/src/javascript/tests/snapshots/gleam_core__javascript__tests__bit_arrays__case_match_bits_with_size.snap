import { toBitArray, bitArraySlice, sizedInt } from "../gleam.mjs";

function go(x) {
  let $ = toBitArray([sizedInt(0x77, 7, true)]);
  if (($.bitSize >= 4) && (($.bitSize >= 6) && ($.bitSize === 7))) {
    let f = bitArraySlice($, 4, 6);
    return f;
  } else {
    return x;
  }
}
