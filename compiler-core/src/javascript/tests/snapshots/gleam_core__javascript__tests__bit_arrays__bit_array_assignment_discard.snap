import { makeError, toBitArray } from "../gleam.mjs";

const FILEPATH = "src/module.gleam";

export function main() {
  let $ = toBitArray([10]);
  if ($.bitSize !== 8) {
    throw makeError(
      "let_assert",
      FILEPATH,
      "my/mod",
      3,
      "main",
      "Pattern match failed, no pattern matched the value.",
      { value: $, start: 18, pattern_start: 29, pattern_end: 44 }
    )
  }
  let number = $.byteAt(0);
  return number;
}
