import { throwError } from "../gleam.mjs";

function go(x) {
  let $ = x + 1;
  if ($ !== 1) {
    throwError(
      "assignment_no_match",
      "my/mod",
      1,
      "go",
      "Assignment pattern did not match",
      { value: $ }
    );
  }
  return $;
}

