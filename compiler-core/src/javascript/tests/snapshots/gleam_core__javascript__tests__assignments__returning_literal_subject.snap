import { makeError } from "../gleam.mjs";

function go(x) {
  let $ = x + 1;
  if ($ !== 1) {
    throw makeError(
      "let_assert",
      "my/mod",
      1,
      "go",
      "Pattern match failed, no pattern matched the value.",
      { value: $ }
    )
  }
  return $;
}
