import { CustomType as $CustomType } from "../gleam.mjs";

export class Teacher extends $CustomType {
  constructor(name, title) {
    super();
    this.name = name;
    this.title = title;
  }
}
export function Person$Teacher(name, title) {
  return new Teacher(name, title);
}
export function Person$isTeacher(value) {
  return value instanceof Teacher;
}

export class Student extends $CustomType {
  constructor(name, age) {
    super();
    this.name = name;
    this.age = age;
  }
}
export function Person$Student(name, age) {
  return new Student(name, age);
}
export function Person$isStudent(value) {
  return value instanceof Student;
}

export function get_name(person) {
  return person.name;
}
