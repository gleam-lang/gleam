----- SOURCE CODE

fn go(x) {
  case x {
    // We want `a` to be defined before the guard check, and
    // `b` to be defined only if the predicate on a matches!
    [a, b] if a == 1 -> a + b
    _ -> 2
  }
}


----- COMPILED JAVASCRIPT
function go(x) {
  if (x.hasLength(0)) {
    return 2;
  } else {
    if (x.tail.hasLength(0)) {
      return 2;
    } else {
      if (x.tail.tail.hasLength(0)) {
        let a = x.head;
        if (a === 1) {
          let b = x.tail.head;
          return a + b;
        } else {
          return 2;
        }
      } else {
        return 2;
      }
    }
  }
}
