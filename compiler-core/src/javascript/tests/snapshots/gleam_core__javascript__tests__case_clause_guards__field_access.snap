import { CustomType as $CustomType } from "../gleam.mjs";

class Person extends $CustomType {
  constructor(username, name, age) {
    super();
    this.username = username;
    this.name = name;
    this.age = age;
  }
}

export function main() {
  let given_name = "jill";
  let person = new Person("credo", "jack", 32);
  if (given_name === person.name) {
    let name = given_name;
    return "it's jack";
  } else {
    let name = given_name;
    return "it's " + name;
  }
}

