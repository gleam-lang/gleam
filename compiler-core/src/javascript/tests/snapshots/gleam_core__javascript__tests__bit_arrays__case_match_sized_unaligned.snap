import { bitArraySliceToInt } from "../gleam.mjs";

function go(x) {
  if ((x.bitSize >= 17) && (x.bitSize === 24)) {
    let a = bitArraySliceToInt(x, 0, 17, true, false);
    let b = bitArraySliceToInt(x, 17, 24, true, false);
    return b * 2;
  } else {
    return 1;
  }
}
