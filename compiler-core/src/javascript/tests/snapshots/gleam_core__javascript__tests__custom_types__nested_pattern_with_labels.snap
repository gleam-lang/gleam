----- COMPILED JAVASCRIPT
import { CustomType as $CustomType } from "../gleam.mjs";

export class Box extends $CustomType {
  constructor(a, b) {
    super();
    this.a = a;
    this.b = b;
  }
  
  withFields(fields) {
    return new this.constructor(
      'a' in fields ? fields.a : this.a,
      'b' in fields ? fields.b : this.b,
    );
  }
}

function go(x) {
  if (x instanceof Box && x.b instanceof Box) {
    let a = x.b.a;
    let b = x.b.b;
    return a + b;
  } else {
    return 1;
  }
}
