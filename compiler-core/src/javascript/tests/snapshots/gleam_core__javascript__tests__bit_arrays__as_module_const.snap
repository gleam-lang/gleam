import { toBitArray, sizedInt, stringBits, sizedFloat } from "../gleam.mjs";

export const data = /* @__PURE__ */ toBitArray([
  0x1,
  2,
  sizedInt(2, 16, true),
  sizedInt(0x4, 32, true),
  sizedInt(-1, 32, true),
  stringBits("Gleam"),
  sizedFloat(4.2, 64, true),
  sizedFloat(4.2, 32, true),
  /* @__PURE__ */ toBitArray([
    /* @__PURE__ */ toBitArray([1, 2, 3]).buffer,
    stringBits("Gleam"),
    1024,
  ]).buffer,
]);
