import { throwError } from "../gleam.js";

function go(x) {
  if (x.isOk()) {
    return 1;
  } else if (!x.isOk()) {
    return 0;
  } else {
    throwError(
      "badmatch",
      "my/mod",
      3,
      "go",
      "Pattern did not match",
      { values: [x] }
    );
  }
