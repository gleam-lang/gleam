----- COMPILED JAVASCRIPT
import { bitArraySliceToInt } from "../gleam.mjs";

function go(x) {
  if ((x.bitSize >= 16) && (x.bitSize === 24)) {
    let a = bitArraySliceToInt(x, 0, 16, true, false);
    let b = x.byteAt(2);
    return a + b;
  } else {
    return 1;
  }
}
