pub type Wibble {
  Wibble
  Wobble(int: Int)
}

const wibble = Wibble

pub fn main() {
  echo wibble
  // This 'wibble' shadows the const
  let wibble = Wobble(42)
  case wibble {
    // This matches the local variable, not the const
    Wobble(_) -> wibble
  }
}


