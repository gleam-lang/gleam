-module(my@mod).
-compile([no_auto_import, nowarn_unused_vars, nowarn_unused_function, nowarn_nomatch]).

-export([go/2]).

-spec go(integer(), integer()) -> integer().
go(X, Y) ->
    m:f(X, Y).

-spec x() -> integer().
x() ->
    m:f(1, 2),
    m:f(4, 3).
