-spec myfun(integer()) -> {ok, integer()} | {error, any()}.
myfun(Mt) ->
    case Mt of
        1 ->
            _pipe = 1,
            {ok, _pipe};

        _ ->
            _pipe = 1,
            {ok, _pipe}
    end,
    _pipe@1 = 1,
